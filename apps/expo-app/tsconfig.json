{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "exactOptionalPropertyTypes": false,
    "allowJs": true,
    "moduleDetection": "force",
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "target": "ESNext",
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    ".expo"
  ],
  "include": [
    "index.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    "wdyr.js"
  ],
  "extends": "expo/tsconfig.base"
}
