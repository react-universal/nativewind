{
  "compilerOptions": {
    "jsx": "preserve",
    "typeRoots": [
      "node_modules/@types"
    ],
    "module": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "allowJs": true,
    "strict": false,
    "skipLibCheck": true,
    "disableSourceOfProjectReferenceRedirect": true,
    "declarationMap": false,
    "skipDefaultLibCheck": true
  }
}