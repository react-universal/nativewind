{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Twin run",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost:5173",
      "enableDWARF": true
    },
    {
      "name": "2 - Launch Twin run",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "vite",
      ],
      "autoAttachChildProcesses": true,
      "restart": true,
      "env": {
        "NO_COLOR": "1"
      },
      "enableDWARF": true,
      "serverReadyAction": {
        "action": "debugWithChrome",
        "killOnServerStop": true,
        "pattern": "localhost:.*m([0-9]+)",
        "uriFormat": "http://localhost:%s",
        "webRoot": "${workspaceFolder}"
      }
    }
  ]
}