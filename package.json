{
  "name": "native-twin",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin.git"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "workspaces": [
    "packages/*",
    "packages/dev-tools/webui",
    "sites/twin.run",
    "apps/*"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:release": "npx turbo run build && yarn libs:foreach pack && yarn libs:foreach exec \"mkdir _release && tar zxvf package.tgz --directory _release && rm package.tgz\"",
    "build:watch": "turbo run build:watch --concurrency 15",
    "clean": "node ./scripts/clean.mjs",
    "docgen": "yarn libs:foreach exec npx docgen && node ./scripts/docs.mjs",
    "docs:build": "turbo run docs:build",
    "example:expo": "yarn workspace expo-app run start",
    "extension:dev": "turbo run vscode:dev",
    "format": "prettier --write packages apps examples \"**/*.{js,ts,tsx,md}\"",
    "libs:foreach": "yarn workspaces foreach --all --no-private --verbose",
    "lint": "nx run-many -t lint --all",
    "nextjs:pages:dev": "turbo run nextjs:pages:dev",
    "release": "yarn build:release && changeset publish",
    "test": "turbo run test"
  },
  "dependencies": {
    "@effect/typeclass": "catalog:effect",
    "effect": "catalog:effect",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "catalog:",
    "react-native-web": "catalog:"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@effect/build-utils": "^0.7.8",
    "@effect/experimental": "catalog:effect",
    "@nx/eslint": "^20.1.3",
    "@nx/eslint-plugin": "^20.1.3",
    "@nx/jest": "^20.1.3",
    "@nx/js": "20.1.3",
    "@testing-library/react-native": "^12.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/chai": "4.3.5",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.10.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "chai": "4.3.7",
    "concurrently": "^7.6.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "mocha": "^10.2.0",
    "nx": "^20.1.3",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch",
      "babel-plugin-annotate-pure-calls@0.4.0": "patches/babel-plugin-annotate-pure-calls@0.4.0.patch"
    }
  }
}
