{
  "name": "native-twin",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin.git"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "workspaces": [
    "packages/*",
    "packages/dev-tools/webui",
    "sites/twin.run",
    "apps/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "build:release": "npx turbo run build && yarn libs:foreach pack && yarn libs:foreach exec \"mkdir _release && tar zxvf package.tgz --directory _release && rm package.tgz\"",
    "clean": "node ./scripts/clean.mjs",
    "docgen": "yarn libs:foreach exec npx docgen && node ./scripts/docs.mjs",
    "docs:build": "turbo run docs:build",
    "example:expo": "yarn workspace expo-app run start",
    "extension:dev": "turbo run vscode:dev",
    "format": "prettier --write packages apps examples \"**/*.{js,ts,tsx,md}\"",
    "libs:foreach": "yarn workspaces foreach --all --no-private --verbose",
    "lint": "turbo run lint",
    "nextjs:pages:dev": "turbo run nextjs:pages:dev",
    "release": "yarn build:release && changeset publish",
    "test": "turbo run test"
  },
  "dependencies": {
    "effect": "^3.10.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-native": "0.74.5",
    "react-native-web": "~0.19.10",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@effect/build-utils": "^0.7.8",
    "@effect/experimental": "^0.32.12",
    "@rollup/plugin-typescript": "^12.1.1",
    "@testing-library/react-native": "^12.9.0",
    "@types/chai": "4.3.5",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.10.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack": "^5.28.5",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "chai": "4.3.7",
    "concurrently": "^7.6.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "lightningcss": "^1.28.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "mocha": "^10.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "turbo": "^2.3.3",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.5.3"
}
