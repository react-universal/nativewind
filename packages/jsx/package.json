{
  "name": "@native-twin/jsx",
  "version": "7.0.0-next.3",
  "description": "Native Twin JSX runtime",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/jsx"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "exports": {
    ".": "./build/cjs/index.js",
    "./jsx-dev-runtime": "./build/cjs/jsx-dev-runtime.js",
    "./types": "./types.d.ts"
  },
  "main": "./build/cjs/index.js",
  "files": [
    "build",
    "jsx-dev-runtime",
    "jsx-runtime",
    "LICENSE",
    "sheet",
    "src",
    "testing-library",
    "rn-svg.d.ts",
    "third-party-declarations.d.ts",
    "types.d.ts",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build-esm && yarn build-annotate && yarn build-cjs && build-utils pack-v2",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --plugins @babel/plugin-syntax-jsx --out-dir build/esm --source-maps",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/plugin-syntax-jsx --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-esm": "tsc -b tsconfig.build.json",
    "build:watch": "tsc -b tsconfig.dev.json -w",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand --silent false --watch",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "~18.2.79"
  },
  "peerDependencies": {
    "react": "~18.2.0",
    "react-native-reanimated": "*"
  },
  "optionalDependencies": {
    "@jest/globals": "*",
    "jest": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "effect": {
    "generateExports": {
      "include": [
        "*.ts"
      ]
    }
  }
}
