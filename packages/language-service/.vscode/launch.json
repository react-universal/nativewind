// A launch configuration that compiles the extension and then opens it inside a new window
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Build language service",
      "type": "node",
      "request": "attach",
      "sourceMaps": true,
      "presentation": {
        "hidden": true,
        "group": "",
        "order": 1
      },
      "preLaunchTask": "Build watch language service",
      "outFiles": [
        "${workspaceRoot}/build/**/*.js"
      ]
    }
  ],
}