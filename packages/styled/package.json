{
  "name": "@native-twin/styled",
  "version": "7.0.0-next.3",
  "description": "[DEPRECATED] Styled components for css in react native",
  "keywords": [
    "react-native",
    "tailwindcss",
    "tailwind-ui",
    "react-native-web",
    "universal-apps"
  ],
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/styled"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "type": "module",
  "files": [
    "build",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "src",
    "styled"
  ],
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --plugins @babel/plugin-syntax-jsx --out-dir build/esm --source-maps",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/plugin-syntax-jsx --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-esm": "tsc -b tsconfig.build.json",
    "bundle": "yarn clean && yarn twin-build",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "eslint \"./**/*.{ts,tsx}\"",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "react-is": "^18.2.0",
    "stable-hash": "0.0.3"
  },
  "devDependencies": {
    "@types/react-is": "^17.0.3"
  },
  "peerDependencies": {
    "@types/react": "~18.2.79",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-native": "0.74.5",
    "react-native-web": "~0.19.10"
  },
  "optionalDependencies": {
    "jest": "*"
  },
  "packageManager": "pnpm@9.14.2",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "gitHead": "4c26121d296e38153faa7dd196229db1a71e075d"
}
