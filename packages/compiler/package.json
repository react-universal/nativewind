{
  "name": "@native-twin/compiler",
  "version": "7.0.0-next.3",
  "description": "Common compilers for native twin",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/compiler"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "exports": {
    ".": {
      "types": "./build/dts/index.d.ts",
      "import": "./build/esm/index.js",
      "default": "./build/cjs/index.js"
    },
    "./node": {
      "types": "./build/dts/node.d.ts",
      "import": "./build/esm/node.js",
      "default": "./build/cjs/node.js"
    },
    "./server": {
      "types": "./build/dts/server.d.ts",
      "import": "./build/esm/server.js",
      "default": "./build/cjs/server.js"
    },
    "./TwinEnv": {
      "types": "./build/dts/TwinEnv.d.ts",
      "import": "./build/esm/TwinEnv.js",
      "default": "./build/cjs/TwinEnv.js"
    }
  },
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/dts/index.d.ts",
  "files": [
    "build",
    "LICENSE",
    "src",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "npx twin pack-dev",
    "build:watch": "npx twin pack-dev -w",
    "clean": "rm -rf build",
    "gen:jsdocs": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@effect/platform": "^0.69.3",
    "@effect/platform-node": "^0.64.4",
    "@effect/platform-node-shared": "^0.19.3",
    "@effect/printer-ansi": "^0.38.0",
    "@effect/sql": "^0.20.9",
    "@effect/sql-sqlite-node": "^0.21.9",
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*",
    "chokidar": "^4.0.1",
    "code-block-writer": "^13.0.1",
    "effect": "^3.10.0",
    "fast-check": "^3.21.0",
    "jiti": "^2.4.0",
    "js-beautify": "^1.15.1",
    "micromatch": "4.0.5",
    "sucrase": "^3.35.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@effect/typeclass": "^0.29.19",
    "@types/connect": "3.4.38",
    "@types/js-beautify": "^1",
    "@types/micromatch": "^4.0.4",
    "babel-plugin-tester": "^11.0.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "effect": {
    "generateExports": {
      "include": [
        "*.ts",
        "programs/*.ts"
      ]
    }
  }
}
