{
  "name": "@native-twin/compiler",
  "version": "7.0.0-next.3",
  "description": "Common compilers for native twin",
  "homepage": "https://github.com/react-universal/native-twin#readme",
  "bugs": {
    "url": "https://github.com/react-universal/native-twin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/native-twin",
    "directory": "packages/compiler"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/esm/index.js",
      "require": "./build/index.js",
      "default": "./build/esm/index.js"
    },
    "./node": {
      "types": "./build/node/index.d.ts",
      "import": "./build/esm/node/index.js",
      "require": "./build/node/index.js",
      "default": "./build/esm/node/index.js"
    },
    "./programs/*": {
      "import": "./build/esm/node/programs/*.js",
      "require": "./build/node/programs/*.js",
      "default": "./build/esm/node/programs/*.js"
    }
  },
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "LICENSE",
    "src",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "twin build",
    "build:watch": "twin build -w",
    "clean": "rm -rf build",
    "gen:jsdocs": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@babel/standalone": "^7.24.2",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@effect/cli": "^0.48.3",
    "@effect/platform": "^0.69.3",
    "@effect/platform-node": "^0.64.4",
    "@effect/platform-node-shared": "^0.19.3",
    "@effect/printer-ansi": "^0.38.0",
    "@effect/sql": "^0.20.9",
    "@effect/sql-sqlite-node": "^0.21.9",
    "@effect/typeclass": "^0.29.0",
    "@native-twin/core": "workspace:*",
    "@native-twin/css": "workspace:*",
    "@native-twin/helpers": "workspace:*",
    "@native-twin/jsx": "workspace:*",
    "@native-twin/preset-tailwind": "workspace:*",
    "code-block-writer": "^13.0.1",
    "effect": "^3.10.0",
    "fast-check": "^3.21.0",
    "jiti": "^2.4.0",
    "js-beautify": "^1.15.1",
    "micromatch": "4.0.5",
    "sucrase": "^3.35.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@effect/language-service": "^0.2.0",
    "@types/babel__helper-module-imports": "^7.18.3",
    "@types/babel__standalone": "^7.1.9",
    "@types/connect": "3.4.38",
    "@types/css-tree": "^2.3.8",
    "@types/js-beautify": "^1",
    "@types/micromatch": "^4.0.4",
    "babel-plugin-tester": "^11.0.4"
  },
  "optionalDependencies": {
    "@native-twin/cli": "workspace:*",
    "better-docs": "*",
    "jest": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  }
}
